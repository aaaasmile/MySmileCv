
<h3><%= @identpicture.foto_filename %></h3>
<img src="<%= url_for(:action => "picture", :id => @identpicture.id) %>"/>

<br/>

<%= link_to(edit_identpicture_path(@identpicture), class: 'btn btn-default', aria:{label: 'Edit'} ) do %>
  <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
<% end %>
<%= link_to(identpictures_path, class: 'btn btn-default', aria:{label: 'Back'} ) do %>
  <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
<% end %>

